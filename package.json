{
  "name": "icct-smart-attendance-system",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "test:export": "node scripts/test-export.js",
    "dev": "node server.js",
    "dev:next": "next dev",
    "dev:silent": "next dev --no-open",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "start:next": "next start",
    "lint": "next lint",
    "test:performance": "node scripts/test-performance.js",
    "test:performance:help": "node scripts/test-performance.js --help",
    "setup:performance": "node scripts/setup-performance.js",
    "monitor:performance": "node scripts/performance-monitor.js",
    "monitor:api": "node scripts/performance-monitor.js --api-only",
    "monitor:memory": "node scripts/performance-monitor.js --memory-only",
    "monitor:render": "node scripts/performance-monitor.js --render-only",
    "performance:report": "node scripts/performance-monitor.js --generate-report",
    "analyze:bundle": "npm run build && npx @next/bundle-analyzer",
    "profile:memory": "node --inspect scripts/performance-monitor.js --memory-profile",
    "check:db": "node scripts/check-database.js",
    "db:start": "docker-compose up -d postgres",
    "db:stop": "docker-compose down",
    "db:reset": "docker-compose down -v && docker-compose up -d postgres",
    "db:migrate": "npx prisma migrate dev",
    "db:generate": "npx prisma generate",
    "test:email": "node scripts/test-email-functionality.js",
    "test:email:help": "node scripts/test-email-functionality.js --help",
    "prepare": "husky",
    "precommit:secrets": "lint-staged",
    "perf:test": "node scripts/test-performance-optimizations.js",
    "perf:monitor": "node -e \"console.log('Performance monitoring available at /api/performance/monitor')\"",
    "perf:clear": "node -e \"console.log('Clear performance data via POST /api/performance/monitor')\"",
    "security:test": "node scripts/test-security-enhancements.js",
    "security:audit": "node -e \"console.log('Security audit available at /api/security/monitor')\"",
    "security:logs": "node -e \"console.log('Audit logs available at /api/audit/logs')\"",
    "redis:test": "node scripts/test-redis-connection.js",
    "redis:status": "docker exec icct-sas-redis redis-cli ping"
  },
